{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 12
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
    "prettier",
    "prefer-arrow"
  ],
  "rules": { "prettier/prettier": "error", "import/no-cycle": "off", "no-unused-vars": "off", "no-param-reassign": [2, { "props": false }] }
}
